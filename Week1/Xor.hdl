/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
Not(in=a,out=notA);
Not(in=b,out=notB);
And(a=notA, b=b,out=and1);
And(a=notB, b=a,out=and2);
Or(a=and1,b=and2,out=out);
}